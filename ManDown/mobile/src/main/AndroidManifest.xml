<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.diyar.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OldMainActivity" />
        <activity android:name=".GameActivity" />
        <activity android:name=".GameMenuActivity" />
        <activity android:name=".WhackABeerMenuActivity" />
        <activity android:name=".TightropeWaiterMenuActivity" />
        <activity android:name=".WhackABeerGameActivity" />
        <activity android:name=".TightropeWaiterGameActivity"></activity>
    </application>

</manifest>